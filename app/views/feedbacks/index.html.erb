
  <%= form_with model: @feedback , local: true do |form| %>
  <div class="form-group">
    <p>
      <%= form.label :name %>
      <%= form.text_field :name, class: 'form-control' %>
    </p><br>

    <p>
      <%= form.label :email %>
      <%= form.text_field :email, class: 'form-control' %>
    </p><br>

    <p>
      <%= form.label :message %>
      <%= form.text_area :message, size: "50x5", class: 'form-control' %>
    </p><br>
    <p><%= form.submit class: 'btn btn-success'%></p>
  </div>

  <% end %>
